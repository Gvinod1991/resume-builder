{
  "name": "resume-builder-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@heroicons/react": "^1.0.5",
    "@tailwindcss/forms": "^0.4.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "env-cmd": "^10.1.0",
    "firebase": "^9.6.3",
    "rc-tabs": "^11.10.5",
    "react": "^17.0.2",
    "react-datepicker": "^4.6.0",
    "react-dom": "^17.0.2",
    "react-firebase-hooks": "^5.0.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.1.0",
    "react-scripts": "5.0.0-next.58",
    "react-toastify": "^8.1.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "typescript": "^4.5.3",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "start": "env-cmd -f .env.local react-scripts start",
    "stage:start": "env-cmd -f .env.production react-scripts start",
    "build": "env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/**.{js,jsx,ts,tsx}",
    "lint-and-fix": "eslint src/**/**.{js,jsx,ts,tsx} --fix",
    "prettier:fix": "prettier --write",
    "prettier:check": "prettier --check src/**/**.{js,jsx,ts,tsx,json,css}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-datepicker": "^4.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.7.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.2",
    "postcss": "^8.4.4",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css}": "yarn prettier:fix",
    "*.{js,jsx,ts,tsx}": "yarn lint-and-fix"
  }
}